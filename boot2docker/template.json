{
    "template": {
        "name": "boot2docker", 
        "author": "Jakub Klama <jakub@ixsystems.com>", 
        "description": "boot2docker Docker host for FreeNAS", 
        "created_at": {
            "$date": "2016-02-10T22:45:38"
        }, 
        "updated_at": {
            "$date": "2017-03-09T00:18:55"
        }, 
        "files": true, 
        "fetch": [
            {
                "name": "vmlinuz64", 
                "url": "http://download.freenas.org/vm-templates/boot2docker/vmlinuz64.gz", 
                "sha256": "aece7155f22998117222bea0d868783f8d423b54604f064fd172162667b513bd",
                "size": 3633055,
                "dest": "grub"
            }, 
            {
                "name": "initrd", 
                "url": "http://download.freenas.org/vm-templates/boot2docker/initrd.img.gz", 
                "sha256": "f30614f01440277a7b4b47ed1ed5f79310f46f78d4e8f2e07d8eb13bb255b5b8",
                "size": 43514213,
                "dest": "grub"
            }
        ]
    }, 
    "config": {
        "docker_host": true, 
        "ncpus": 1, 
        "bootloader": "GRUB", 
        "memsize": 2048,
        "minmemsize": 1024,
        "boot_directory": "grub", 
        "autostart": true
    }, 
    "devices": [
        {
            "type": "NIC", 
            "name": "mgmt", 
            "properties": {
                "mode": "MANAGEMENT"
            }
        }, 
        {
            "type": "NIC", 
            "name": "bridge", 
            "properties": {
                "mode": "BRIDGED",
                "bridge": "default"
            }
        }, 
        {
            "type": "VOLUME", 
            "name": "docker", 
            "properties": {
                "type": "VT9P", 
                "auto": true
            }
        }, 
        {
            "type": "VOLUME", 
            "name": "mnt", 
            "properties": {
                "type": "VT9P", 
                "destination": "/mnt", 
                "auto": false
            }
        }
    ], 
    "guest_type": "linux64"
}
