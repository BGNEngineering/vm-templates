        "files": true, 
        "fetch": [
            {
                "name": "vmlinuz64", 
                "url": "http://download.freenas.org/vm-templates/boot2docker/vmlinuz64.gz", 
                "sha256": "5079e23df9df52436c82892333e15f225db99f1a9f33293f1b3c3474e559d8b2", 
                "size": 3504133, 
                "dest": "grub"
            }, 
            {
                "name": "initrd", 
                "url": "http://download.freenas.org/vm-templates/boot2docker/initrd.img.gz", 
                "sha256": "e5e7842c240397f774f8c4578272aa84615cc2bc47522e97dff6397e16726e21", 
                "size": 28805951, 
                "dest": "grub"
            }
