            "name": "bridge", 
            "properties": {
                "mode": "BRIDGED",
                "bridge": "default"
            }
        }, 
        {
            "type": "VOLUME", 
            "name": "docker", 
            "properties": {
                "type": "VT9P", 
                "auto": true
            }
        }, 
        {
            "type": "VOLUME", 
            "name": "mnt", 
            "properties": {
                "type": "VT9P", 
                "destination": "/mnt", 
                "auto": false
            }
        }
    ], 
    "guest_type": "linux64"
