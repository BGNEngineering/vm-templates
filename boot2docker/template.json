{
    "template": {
        "name": "boot2docker", 
        "author": "Jakub Klama <jakub@ixsystems.com>", 
        "description": "boot2docker Docker host for FreeNAS", 
        "created_at": {
            "$date": "2016-02-10T22:45:38"
        }, 
        "updated_at": {
            "$date": "2017-02-21T23:47:55"
        }, 
        "files": true, 
        "fetch": [
            {
                "name": "vmlinuz64", 
                "url": "http://download.freenas.org/vm-templates/boot2docker/vmlinuz64.gz", 
                "sha256": "38708ba3509069dd216a9dbdb0de9c5306e14f8e5733b2fe61146bf6b6a810d8",
                "size": 3632072,
                "dest": "grub"
            }, 
            {
                "name": "initrd", 
                "url": "http://download.freenas.org/vm-templates/boot2docker/initrd.img.gz", 
                "sha256": "956be1b98a24a06825f1182840aaf302a7002df06e250e8bdeb3c51c906bc293",
                "size": 43560677,
                "dest": "grub"
            }
        ]
    }, 
    "config": {
        "docker_host": true, 
        "ncpus": 1, 
        "bootloader": "GRUB", 
        "memsize": 2048,
        "minmemsize": 1024,
        "boot_directory": "grub", 
        "autostart": true
    }, 
    "devices": [
        {
            "type": "NIC", 
            "name": "mgmt", 
            "properties": {
                "mode": "MANAGEMENT"
            }
        }, 
        {
            "type": "NIC", 
            "name": "bridge", 
            "properties": {
                "mode": "BRIDGED",
                "bridge": "default"
            }
        }, 
        {
            "type": "VOLUME", 
            "name": "docker", 
            "properties": {
                "type": "VT9P", 
                "auto": true
            }
        }, 
        {
            "type": "VOLUME", 
            "name": "mnt", 
            "properties": {
                "type": "VT9P", 
                "destination": "/mnt", 
                "auto": false
            }
        }
    ], 
    "guest_type": "linux64"
}
