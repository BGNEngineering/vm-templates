{
    "template": {
        "name": "boot2docker",
        "author": "Jakub Klama <jakub@ixsystems.com>",
        "description": "boot2docker Docker host for FreeNAS",
        "created_at": {"$date": "2016-02-10T22:45:38"},
        "updated_at": {"$date": "2016-07-06T10:55:11"},
        "files": true,
        "fetch": [
            {
                "name": "vmlinuz64",
                "url": "http://download.freenas.org/vm-templates/boot2docker/vmlinuz64.gz",
                "sha256": "5079e23df9df52436c82892333e15f225db99f1a9f33293f1b3c3474e559d8b2",
                "dest": "grub"
            },
            {
                "name": "initrd",
                "url": "http://download.freenas.org/vm-templates/boot2docker/initrd.gz",
                "sha256": "2f77dd4ac0c72dcdce2d94378104b044f1c65d5f60fa3d2014db998137db35f2",
                "dest": "grub"
            }
        ]
    },
    "config": {
        "docker_host": true,
        "ncpus": 1,
        "bootloader": "GRUB",
        "memsize": 2048,
        "boot_directory": "grub",
        "autostart": true
    },
    "devices" : [
        {
            "type": "NIC",
            "name": "mgmt",
            "properties" : {
                "type": "MANAGEMENT"
            }
        },
        {
            "type": "VOLUME",
            "name": "docker",
            "properties": {
                "type": "VT9P",
                "auto": true
            }
        },
        {
            "type": "VOLUME",
            "name": "mnt",
            "properties": {
                "type": "VT9P",
                "destination": "/mnt",
                "auto": false
            }
        }
    ],
    "type": "VM"
}
