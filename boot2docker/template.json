{
    "template": {
        "name": "boot2docker",
        "author": "Jakub Klama <jakub@ixsystems.com>",
        "description": "boot2docker Docker host for FreeNAS",
        "created_at": {"$date": "2016-02-10T22:45:38"},
        "updated_at": {"$date": "2016-06-27T18:05:26"},
        "files": true,
        "fetch": [
            {
                "name": "vmlinuz64",
                "url": "http://download.freenas.org/vm-templates/boot2docker/vmlinuz64.gz",
                "sha256": "590fb14ab19b82154e50bccc59564c10168c9a81e327ba792b8b5cb786b207ad",
                "dest": "grub"
            },
            {
                "name": "initrd",
                "url": "http://download.freenas.org/vm-templates/boot2docker/initrd.gz",
                "sha256": "5816f3f285e50174df132f226d1a47878743af720f8f230360328ac9f1aab86f",
                "dest": "grub"
            }
        ]
    },
    "config": {
        "docker_host": true,
        "ncpus": 1,
        "bootloader": "GRUB",
        "memsize": 2048,
        "boot_directory": "grub",
        "autostart": true
    },
    "devices" : [
        {
            "type": "NIC",
            "name": "mgmt",
            "properties" : {
                "type": "MANAGEMENT"
            }
        },
        {
            "type": "VOLUME",
            "name": "docker",
            "properties": {
                "type": "VT9P",
                "auto": true
            }
        },
        {
            "type": "VOLUME",
            "name": "mnt",
            "properties": {
                "type": "VT9P",
                "destination": "/mnt",
                "auto": false
            }
        }
    ],
    "type": "VM"
}
