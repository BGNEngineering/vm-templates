{
    "template": {
        "name": "boot2docker",
        "author": "Jakub Klama <jakub@ixsystems.com>",
        "description": "boot2docker Docker host for FreeNAS",
        "created_at": {"$date": "2016-02-10T22:45:38"},
        "updated_at": {"$date": "2016-05-28T12:46:44"},
        "files": true,
        "fetch": [
            {
                "name": "vmlinuz64",
                "url": "http://download.freenas.org/vm-templates/boot2docker/vmlinuz64.gz",
                "sha256": "d1ac88a39660c37d934dc6526f03706e6299c567bbe512e5be5664f7bec37568",
                "dest": "grub"
            },
            {
                "name": "initrd",
                "url": "http://download.freenas.org/vm-templates/boot2docker/initrd.gz",
                "sha256": "277d626ad12c395912debda66575657ef8f288c2720d78d9c258bd4542a18d62",
                "dest": "grub"
            }
        ]
    },
    "config": {
        "docker_host": true,
        "ncpus": 1,
        "bootloader": "GRUB",
        "memsize": 2048,
        "boot_directory": "grub"
    },
    "devices" : [
        {
            "type": "NIC",
            "name": "mgmt",
            "properties" : {
                "type": "MANAGEMENT"
            }
        },
        {
            "type": "VOLUME",
            "name": "docker",
            "properties": {
                "type": "VT9P",
                "auto": true
            }
        },
        {
            "type": "VOLUME",
            "name": "mnt",
            "properties": {
                "type": "VT9P",
                "destination": "/mnt",
                "auto": false
            }
        }
    ],
    "type": "VM"
}
