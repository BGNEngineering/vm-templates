{
    "template": {
        "name": "boot2docker", 
        "author": "Jakub Klama <jakub@ixsystems.com>", 
        "description": "boot2docker Docker host for FreeNAS", 
        "created_at": {
            "$date": "2016-02-10T22:45:38"
        }, 
        "updated_at": {
            "$date": "2017-03-04T17:36:55"
        }, 
        "files": true, 
        "fetch": [
            {
                "name": "vmlinuz64", 
                "url": "http://download.freenas.org/vm-templates/boot2docker/vmlinuz64.gz", 
                "sha256": "4d3defe548222cf0defedb34ad84dab52d10e22ad74e4777e74e176a67e510ca",
                "size": 3633055,
                "dest": "grub"
            }, 
            {
                "name": "initrd", 
                "url": "http://download.freenas.org/vm-templates/boot2docker/initrd.img.gz", 
                "sha256": "167c971fdec57e45e2a3cec7315d483cca8795b73fcaaeca75fac89e01d65697",
                "size": 43514213,
                "dest": "grub"
            }
        ]
    }, 
    "config": {
        "docker_host": true, 
        "ncpus": 1, 
        "bootloader": "GRUB", 
        "memsize": 2048,
        "minmemsize": 1024,
        "boot_directory": "grub", 
        "autostart": true
    }, 
    "devices": [
        {
            "type": "NIC", 
            "name": "mgmt", 
            "properties": {
                "mode": "MANAGEMENT"
            }
        }, 
        {
            "type": "NIC", 
            "name": "bridge", 
            "properties": {
                "mode": "BRIDGED",
                "bridge": "default"
            }
        }, 
        {
            "type": "VOLUME", 
            "name": "docker", 
            "properties": {
                "type": "VT9P", 
                "auto": true
            }
        }, 
        {
            "type": "VOLUME", 
            "name": "mnt", 
            "properties": {
                "type": "VT9P", 
                "destination": "/mnt", 
                "auto": false
            }
        }
    ], 
    "guest_type": "linux64"
}
